{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      // "eslint:recommended",
      // "plugin:react/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    // "no-console" : "warn",
    "indent" : ["warn",2]
    // "no-trailing-spaces" : "error",
    // "comma-spacing" : "warn",
    // "comma-style" : "warn",
    // "comma-dangle" : "warn",
    // "eol-last" : "error"
  }
}